{
    "version": 2,
    "builds": [
      {
        "src": "test.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "test.js"
      }
    ],
    "functions": {
        "controllers/*.js": {
          "maxDuration": 600
        },
        "middleware/**/*.js": {
          "maxDuration": 600
        }
      }
  }